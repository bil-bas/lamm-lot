<LAMMLotWindow>:
    id: main_window
    rows: 3

    MDLabel:
        text: 'Location'

    MDDropDownItem
        id: location_picker
        text: 'Lancaster Community Makerspace'
        on_release: main_window.open_location_menu(self)

    MDTextButton:
        text: 'Refresh'
        on_release: main_window.refresh_locations()
